{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["src/scripts"],
    "includePattern": ".+\\.js$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "template": "./node_modules/minami"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "referenceTitle": "OLEOjs"
  }
}
